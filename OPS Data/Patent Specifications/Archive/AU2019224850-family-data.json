            "invention-title": {
              "$": "Treatment of allergic rhinitis in pediatric subjects using a combination of mometasone and olopatadine",
              "@lang": "en"
            }
          },
          "abstract": {
            "@lang": "en",
            "p": {
              "$": "The present invention relates to a method of treating allergic rhinitis in a pediatric subject in need thereof comprising nasally administering to the subject an effective amount of a fixed-dose pharmaceutical composition comprising mometasone or its salt and olopatadine or its salt."
            }
